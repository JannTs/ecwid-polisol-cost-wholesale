{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~~/*": ["./*"],
      "@@/*": ["./*"],
      "~/*": ["./app/*"],
      "@/*": ["./app/*"]
    },
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowJs": true,
    "strict": true
  },
  "exclude": ["node_modules", ".nuxt", "dist"]
}
